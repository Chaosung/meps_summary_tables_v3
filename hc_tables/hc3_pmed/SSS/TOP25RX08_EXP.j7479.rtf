\par 437        /* Compute Z-Scores */
\par 438        DATA ZSCORE (KEEP=DRUG1 DRUG2
\par 439                          TOTAL1 TOTAL2
\par 440                          SE_EXP1 SE_EXP2
\par 441                          STATDIFF SESQRT ZSTAT PVALUE);
\par 442          SET SIGSTAT;
