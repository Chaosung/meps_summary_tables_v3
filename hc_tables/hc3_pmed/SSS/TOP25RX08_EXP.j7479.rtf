\par 444          ARRAY RNK(25)   RANK1-RANK25;
\par 445          ARRAY CSTAT(25) STAT1-STAT25;
\par 446          ARRAY CSE(25)   SE1-SE25;
\par 447
\par 448          DO M=1 TO NSTAT-1;
\par 449             DO J=1 TO (NSTAT-M);
\par 450                DRUG1=RNK(M);
\par 451                TOTAL1=CSTAT(M);
\par 452                SE_EXP1=CSE(M);
