\par 441          ARRAY RNK(10)   RANK1-RANK10;
\par 442          ARRAY CSTAT(10) STAT1-STAT10;
\par 443          ARRAY CSE(10)   SE1-SE10;
\par 444
\par 445          DO M=1 TO NSTAT-1;
\par 446             DO J=1 TO (NSTAT-M);
\par 447                DRUG1=RNK(M);
\par 448                TOTAL1=CSTAT(M);
\par 449                SE_USE1=CSE(M);
