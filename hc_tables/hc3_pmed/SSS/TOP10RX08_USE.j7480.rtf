\par 434        /* Compute Z-Scores */
\par 435        DATA ZSCORE (KEEP=DRUG1 DRUG2
\par 436                          TOTAL1 TOTAL2
\par 437                          SE_USE1 SE_USE2
\par 438                          STATDIFF SESQRT ZSTAT PVALUE);
\par 439          SET SIGSTAT;
